{
  "name": "app-template",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for app-template goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "vite build --mode test && ember exam --path dist",
    "test:ember": "vite build --mode test && ember exam --path dist"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/runtime": "^7.25.6",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.1.0",
    "@embroider/compat": "alpha",
    "@embroider/config-meta-loader": "alpha",
    "@embroider/core": "alpha",
    "@embroider/router": "alpha",
    "@embroider/vite": "alpha",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.1.2",
    "decorator-transforms": "^2.0.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~6.2.2",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-exam": "github:NullVoxPopuli/ember-exam#vite-support-via-path-argument",
    "ember-load-initializers": "^3.0.0",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^9.0.1",
    "ember-qunit": "^9.0.1",
    "ember-resolver": "^13.1.0",
    "ember-source": "~6.2.0",
    "ember-template-lint": "^6.1.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.2",
    "qunit": "^2.19.4",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-prettier": "^5.0.3",
    "tracked-built-ins": "^4.0.0",
    "vite": "^6.1.1"
  },
  "engines": {
    "node": " >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  }
}
